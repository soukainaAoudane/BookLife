<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- Couleur de fond-->
    <solid android:color="#FFFFFF"/>
    <!-- Coins arrondis-->
    <corners android:radius="12dp"/>
    <!-- Bordure légère-->
    <stroke
        android:width="1dp"
        android:color="#DDDDDD"/>
    <!-- Espacement interne-->
    <padding
        android:left="8dp"
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp"/>
</shape>